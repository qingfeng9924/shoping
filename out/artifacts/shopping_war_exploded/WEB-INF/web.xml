<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

    <servlet>
        <servlet-name>ToLoginServlet</servlet-name>
        <servlet-class>com.cn.shopping.html.ToLoginServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ToLoginServlet</servlet-name>
        <url-pattern>/toLogin</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LogServlet</servlet-name>
        <servlet-class>com.cn.shopping.user.UserLoginServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>LogServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ToHomeServlet</servlet-name>
        <servlet-class>com.cn.shopping.html.ToHomeServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ToHomeServlet</servlet-name>
        <url-pattern>/toHome</url-pattern>
    </servlet-mapping>


</web-app>